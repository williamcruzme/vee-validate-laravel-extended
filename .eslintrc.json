{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "func-names": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-console": 0
  }
}
